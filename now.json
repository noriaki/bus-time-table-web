{
  "version": 2,
  "name": "bus-time-table-web",
  "regions": [
    "all"
  ],
  "public": true,
  "alias": [
    "deux-tours-bus.com",
    "deux-tours-bus-timetable.now.sh"
  ],
  "builds": [
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/robots.txt", "dest": "/static/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/static/sitemap.xml" }
  ]
}
