{
  "version": 2,
  "name": "bus-time-table-web",
  "regions": [
    "all"
  ],
  "public": true,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "dest": "/static/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/static/sitemap.xml"
    },
    {
      "src": "/sw.bundle.js",
      "dest": "/static/sw.js"
    }
  ]
}
